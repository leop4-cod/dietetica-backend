					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"var jsonData = pm.response.json();",
									"// Fix: Check for data.access_token directly",
									"if (jsonData.data && jsonData.data.access_token) {",
									"    pm.collectionVariables.set(\"token\", jsonData.data.access_token);",
									"    console.log(\"Token updated successfully\");",
									"    // Capture User ID",
									"    if (jsonData.data.user && jsonData.data.user.id) {",
									"        pm.collectionVariables.set(\"currentUserId\", jsonData.data.user.id);",
									"        console.log(\"User ID captured: \" + jsonData.data.user.id);",
									"    }",
									"} else if (jsonData.access_token) {",
									"    pm.collectionVariables.set(\"token\", jsonData.access_token);",
									"    if (jsonData.user && jsonData.user.id) {",
									"         pm.collectionVariables.set(\"currentUserId\", jsonData.user.id);",
									"    }",
									"} else {",
									"    console.error(\"Login failed or token missing. Response:\", jsonData);",
									"}"
								],
								"type": "text/javascript",
								"packages": {},
								"requests": {}
